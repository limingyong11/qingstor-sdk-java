    public {{$opID}}Output
    {{if eq $belongs "Bucket" -}}
        {{replace $opID "Bucket" "" -1 | camelCase | lowerFirst }}
    {{else -}}
        {{$opID | camelCase | lowerFirst }}
    {{end}}
    (
    {{if eq $operation.Request.URI $newURI}}
        {{- if $hasInput}}

         {{$opID}}Input input
        {{end}}
    {{- else -}}
